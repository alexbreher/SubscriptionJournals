<script>
    var msg = @(TempData["Message"]!=null? Html.Raw(TempData["Message"]) : Html.Raw("undefined"));

    console.log(msg);

    if (msg) {
        if (msg.provider == "sweetAlert") {
            Swal.fire({
                title: msg.title,
                text: msg.text,
                icon: msg.icon
            })
        } else {
            toastr[msg.type](msg.text, msg.title);
        }
    }
</script>
